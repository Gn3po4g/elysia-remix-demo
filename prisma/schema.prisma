generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:data.db"
}

model Post {
  id    BigInt @id @default(autoincrement())
  title String
  body  String
}

model User {
  id    BigInt @id @default(autoincrement())
  name  String
  email String
}